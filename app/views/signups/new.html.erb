<h1>New signup</h1>

<% form_for(@signup) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :partner_id %><br />
    <%= f.text_field :partner_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :is_optin %><br />
    <%= f.text_field :is_optin %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', signups_path %>
