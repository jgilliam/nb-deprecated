<div id="footer_container" align="center">
<div id="footer">
	<table border="0" cellspacing="0" cellpadding="0">
		<tr>
			<td valign="middle">
				<% if current_government.has_logo? %>
					<div id="footer_image"><a href="/"><%= image_tag current_government.logo.url(:icon_96), :border => 0 %></a></div>
				<% end %>
			</td>
			<td valign="middle"><%= blurb("footer") %></td>
		</tr>
	</table>
</div>
</div>